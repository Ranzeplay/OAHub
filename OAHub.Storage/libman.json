{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "font-awesome@5.14.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "js/all.js",
        "js/all.min.js"
      ]
    },
    {
      "library": "dropify@0.2.2",
      "destination": "wwwroot/lib/dropify/",
      "files": [
        "dist/css/dropify.css",
        "dist/css/dropify.min.css",
        "dist/js/dropify.js",
        "dist/js/dropify.min.js",
        "dist/fonts/dropify.eot",
        "dist/fonts/dropify.svg",
        "dist/fonts/dropify.ttf",
        "dist/fonts/dropify.woff"
      ]
    },
    {
      "library": "carbon-components@10.17.0",
      "destination": "wwwroot/lib/carbon-components/",
      "files": [
        "css/carbon-components.css",
        "css/carbon-components.css.map",
        "css/carbon-components.min.css",
        "css/carbon-components.min.css.map",
        "scripts/carbon-components.js",
        "scripts/carbon-components.min.js"
      ]
    }
  ]
}